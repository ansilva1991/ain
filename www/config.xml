    <preference name="StatusBarBackgroundColor" value="#DC9929" />
    <preference name="KeyboardShrinksView" value="true" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.0.10" />
    <gap:config-file overwrite="true" parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>We are using your photos and camera for profile photos.</string>
    </gap:config-file>
    <gap:config-file overwrite="true" parent="NSCameraUsageDescription" platform="ios">
        <string>We are using your photos and camera for profile photos.</string>
    </gap:config-file>
    <preference name="android-build-tool" value="gradle" />
